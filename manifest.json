{
  "manifest_version": 2,
  "name": "YM Bot Inject Extenstion",
  "version": "1.0.0",
  "description": "A Support Extension to increase the Chatbot usability",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "page_action": {
    "default_icon": "icon16.png",
    "default_title": "YM Bot Inject Extension"
  },
  "background": {
    "scripts": ["event-page.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "cookies",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ]
}
